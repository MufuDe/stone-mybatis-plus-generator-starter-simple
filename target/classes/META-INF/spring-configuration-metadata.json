      "description": "是否打开输出目录",
      "sourceType": "com.stone.custom.properties.CustomGlobalConfigProperties",
      "defaultValue": false
    },
    {
      "name": "stone.global.enable-swagger",
      "type": "java.lang.Boolean",
      "description": "开启 swagger 模式（默认 false 与 springdoc 不可同时使用）",
      "sourceType": "com.stone.custom.properties.CustomGlobalConfigProperties",
      "defaultValue": false
    },
    {
      "name": "stone.global.output-dir",
      "type": "java.lang.String",
      "description": ".java 文件的存放位置",
      "sourceType": "com.stone.custom.properties.CustomGlobalConfigProperties"
    },
    {
      "name": "stone.package.controller",
      "type": "java.lang.String",
      "description": "指定控制器包名",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "controller"
    },
    {
      "name": "stone.package.entity",
      "type": "java.lang.String",
      "description": "指定实体包名",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "entity"
    },
    {
      "name": "stone.package.mapper",
      "type": "java.lang.String",
      "description": "指定mapper接口包名",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "mapper"
    },
    {
      "name": "stone.package.module-name",
      "type": "java.lang.String",
      "description": "指定父包模块名称",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": ""
    },
    {
      "name": "stone.package.other",
      "type": "java.lang.String",
      "description": "指定自定义文件包名",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "other"
    },
    {
      "name": "stone.package.parent",
      "type": "java.lang.String",
      "description": "指定父包名。如果为空，将下面子包名必须写全部， 否则就只需写子包名",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "com.baomidou"
    },
    {
      "name": "stone.package.service",
      "type": "java.lang.String",
