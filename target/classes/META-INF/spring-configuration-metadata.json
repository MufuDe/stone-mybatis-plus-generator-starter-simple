      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "service.impl"
    },
    {
      "name": "stone.package.xml",
      "type": "java.lang.String",
      "description": "指定xml包名",
      "sourceType": "com.stone.custom.properties.CustomPackageConfigProperties",
      "defaultValue": "mapper.xml"
    },
    {
      "name": "stone.strategy.disable-sql-filter",
      "type": "java.lang.Boolean",
      "description": "启用sql过滤，语法不能支持使用sql过滤表的话，可以考虑关闭此开关. @since 3.3.1",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties",
      "defaultValue": false
    },
    {
      "name": "stone.strategy.enable-capital-mode",
      "type": "java.lang.Boolean",
      "description": "是否大写命名（默认 false）",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties",
      "defaultValue": false
    },
    {
      "name": "stone.strategy.enable-schema",
      "type": "java.lang.Boolean",
      "description": "启用 schema 默认 false",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties",
      "defaultValue": false
    },
    {
      "name": "stone.strategy.enable-skip-view",
      "type": "java.lang.Boolean",
      "description": "是否跳过视图（默认 false）",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties",
      "defaultValue": false
    },
    {
      "name": "stone.strategy.exclude",
      "type": "java.util.Set<java.lang.String>",
      "description": "需要排除的表名，允许正则表达式<br\/> 当{@link #disableSqlFilter}为false时，正则表达式无效.",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties"
    },
    {
      "name": "stone.strategy.field-prefix",
      "type": "java.util.Set<java.lang.String>",
      "description": "过滤字段前缀 example: addFieldPrefix(\"is_\") result: is_deleted -> deleted",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties"
    },
    {
      "name": "stone.strategy.field-suffix",
      "type": "java.util.Set<java.lang.String>",
      "description": "过滤字段后缀 example: addFieldSuffix(\"_flag\") result: deleted_flag -> deleted",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties"
    },
    {
      "name": "stone.strategy.include",
      "type": "java.util.Set<java.lang.String>",
      "description": "需要包含的表名，允许正则表达式（与exclude二选一配置）<br\/> 当{@link #disableSqlFilter}为false时，正则表达式无效.",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties"
    },
    {
      "name": "stone.strategy.table-prefix",
      "type": "java.util.Set<java.lang.String>",
      "description": "过滤表前缀 example: addTablePrefix(\"t_\") result: t_simple -> Simple",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties"
    },
    {
      "name": "stone.strategy.table-suffix",
      "type": "java.util.Set<java.lang.String>",
      "description": "过滤表后缀 example: addTableSuffix(\"_0\") result: t_simple_0 -> Simple",
      "sourceType": "com.stone.custom.properties.CustomStrategyConfigProperties"
    },
    {
      "name": "stone.template.controller",
